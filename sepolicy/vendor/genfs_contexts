# DT2W
genfscon proc /sys/dev/dt2w u:object_r:dt2w_ctrl:s0

# LEDs
genfscon sysfs /devices/platform/soc/200f000.qcom,spmi/spmi-0/spmi0-02/200f000.qcom,spmi:qcom,pmi8950@2:qcom,leds@a200/leds/button-backlight	u:object_r:sysfs_leds:s0
genfscon sysfs /devices/platform/soc/200f000.qcom,spmi/spmi-0/spmi0-02/200f000.qcom,spmi:qcom,pmi8950@2:qcom,qpnp-smbcharger/leds		u:object_r:sysfs_leds:s0
genfscon sysfs /devices/platform/soc/soc:flashlight/leds/flashlight-front	u:object_r:sysfs_leds:s0
